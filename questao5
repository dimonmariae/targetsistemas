# tÃ©cnica 5

def inverter_string(string):
    string_invertida = ""
    
    for i in range(len(string) - 1, -1, -1):
        string_invertida += string[i]
    print(f'Texto invertido: {string_invertida}')

entrada_string = str(input('Digite um texto para inverte-lo: '))
resultado = inverter_string(entrada_string)

resultado
